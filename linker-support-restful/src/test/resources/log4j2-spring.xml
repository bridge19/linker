<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">

  <!-- appender配置 -->
  <Appenders>
    <Console name="Console" target="SYSTEM_OUT">
      <PatternLayout pattern="[%d][%t][%p][%c:%L] %m%n"/>
    </Console>
  </Appenders>
  <Loggers>
    <Logger name="io.birdge.linker" level="INFO"/>
    <Root level="info">
      <AppenderRef ref="Console"/>
    </Root>
  </Loggers>
</Configuration>